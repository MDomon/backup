
public class Computer {
	//フィールド
	private String name;
	private int price;
	private String color;
	private String marketName;

	//コンストラクタ
	public Computer(String name,int price,String color,String marketName){
		this.name = name;
		this.price = price;
		this.color = color;
		this.marketName = marketName;
	}

	//ゲッター
	public String getName(){
		return this.name;
	}
	public int getPrice(){
		return this.price;
	}
	public String color(){
		return this.color;
	}
	public String marketName(){
		return this.marketName;
	}
}
