
public class Book extends TangibleAsset{
	//フィールド
	private String isbn;

	//コンストラクタ
	public Book(String name,int price,String color,String isbn){
		this.isbn = isbn;
	}

	//ゲッター
	public String getName(){
		return this.name;
	}
	public int getPrice(){
		return this.price;
	}
	public String getColor(){
		return this.color;
	}
	public String getIsbn(){
		return this.isbn;
	}
}
